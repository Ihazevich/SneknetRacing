        <TextBox x:Name="textBox1_Copy22" HorizontalAlignment="Left" Margin="558,359,0,0" Text="{Binding Path=WheelSlip[1]}" TextWrapping="NoWrap" VerticalAlignment="Top" Width="50"/>
