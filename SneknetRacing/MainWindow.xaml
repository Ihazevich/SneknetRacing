        <Label x:Name="label2_Copy1" Content="SA" HorizontalAlignment="Left" Margin="445,157,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2_Copy2" Content="WSp" HorizontalAlignment="Left" Margin="435,180,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2_Copy3" Content="WSl" HorizontalAlignment="Left" Margin="435,203,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2_Copy4" Content="SP" HorizontalAlignment="Left" Margin="445,263,0,0" VerticalAlignment="Top"/>
