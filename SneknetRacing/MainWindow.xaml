        <Label x:Name="label2_Copy11" Content="SA" HorizontalAlignment="Left" Margin="615,158,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2_Copy12" Content="WSp" HorizontalAlignment="Left" Margin="613,181,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2_Copy13" Content="WSl" HorizontalAlignment="Left" Margin="615,203,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2_Copy14" Content="SP" HorizontalAlignment="Left" Margin="614,263,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2_Copy15" Content="SV" HorizontalAlignment="Left" Margin="614,286,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2_Copy16" Content="SA" HorizontalAlignment="Left" Margin="614,309,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2_Copy17" Content="WSp" HorizontalAlignment="Left" Margin="613,332,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2_Copy18" Content="WSl" HorizontalAlignment="Left" Margin="613,355,0,0" VerticalAlignment="Top"/>
