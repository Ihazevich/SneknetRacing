        <TextBox x:Name="textBox1_Copy10" HorizontalAlignment="Left" Margin="558,207,0,0" Text="{Binding Path=WheelSlip[3]}" TextWrapping="NoWrap" VerticalAlignment="Top" Width="50"/>
        <TextBox x:Name="textBox1_Copy12" HorizontalAlignment="Left" Margin="473,290,0,0" Text="{Binding Path=SuspensionVelocity[0]}" TextWrapping="NoWrap" VerticalAlignment="Top" Width="50"/>
        <TextBox x:Name="textBox1_Copy13" HorizontalAlignment="Left" Margin="473,267,0,0" Text="{Binding Path=SuspensionPosition[0]}" TextWrapping="NoWrap" VerticalAlignment="Top" Width="50"/>
        <TextBox x:Name="textBox1_Copy14" HorizontalAlignment="Left" Margin="473,313,0,0" Text="{Binding Path=SuspensionAcceleration[0]}" TextWrapping="NoWrap" VerticalAlignment="Top" Width="50"/>
        <TextBox x:Name="textBox1_Copy15" HorizontalAlignment="Left" Margin="473,336,0,0" Text="{Binding Path=WheelSpeed[0]}" TextWrapping="NoWrap" VerticalAlignment="Top" Width="50"/>
        <TextBox x:Name="textBox1_Copy16" HorizontalAlignment="Left" Margin="473,359,0,0" Text="{Binding Path=WheelSlip[0]}" TextWrapping="NoWrap" VerticalAlignment="Top" Width="50"/>
        <TextBox x:Name="textBox1_Copy18" HorizontalAlignment="Left" Margin="558,290,0,0" Text="{Binding Path=SuspensionVelocity[1]}" TextWrapping="NoWrap" VerticalAlignment="Top" Width="50"/>
        <TextBox x:Name="textBox1_Copy19" HorizontalAlignment="Left" Margin="558,267,0,0" Text="{Binding Path=SuspensionPosition[1]}" TextWrapping="NoWrap" VerticalAlignment="Top" Width="50"/>
        <TextBox x:Name="textBox1_Copy20" HorizontalAlignment="Left" Margin="558,313,0,0" Text="{Binding Path=SuspensionAcceleration[1]}" TextWrapping="NoWrap" VerticalAlignment="Top" Width="50"/>
